{
    "name": "sertxudeveloper/laravel-counters",
    "description": "Manage counters with year and series.",
    "keywords": ["laravel", "counters", "series", "years", "invoice"],
    "homepage": "https://github.com/sertxudeveloper/laravel-counters",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Peris",
            "email": "speris@grupoperis.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\Counters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\Counters\\CounterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
